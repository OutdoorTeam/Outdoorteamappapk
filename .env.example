# Environment Variables Template
NODE_ENV=development

# Database
VITE_SUPABASE_URL=https://YOUR-STAGING-PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
DATA_DIRECTORY=./data

# Authentication
JWT_SECRET=your-secret-key-change-in-production

# Server Configuration
PORT=3001

# Rate Limiting (set to true to disable during builds)
DISABLE_RATE_LIMITING=false

# Internal Security
INTERNAL_API_KEY=your-internal-api-key
INTERNAL_IPS=127.0.0.1,::1

# Push Notifications (run `npm run generate-vapid` to generate)
VAPID_PUBLIC_KEY=YOUR_PUBLIC_KEY_HERE
VAPID_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE
VAPID_EMAIL=admin@outdoorteam.com

# Build Configuration (for instance.app builds)
BUILD_MODE=false
INSTANCE_APP_BUILD=false
